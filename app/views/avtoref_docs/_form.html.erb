<% form_for @avtoref_doc do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :adoc_file_name %><br />
    <%= f.text_field :adoc_file_name %>
  </p>
  <p>
    <%= f.label :adoc_content_type %><br />
    <%= f.text_field :adoc_content_type %>
  </p>
  <p>
    <%= f.label :adoc_file_size %><br />
    <%= f.text_field :adoc_file_size %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
